{
  "name": "@repo/user-interfaces",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "tsdown -- --watch",
    "build": "tsdown",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\""
  },
  "main": "./dist/entry.cjs",
  "module": "./dist/entry.mjs",
  "types": "./dist/entry.d.mts",
  "files": [
    "./dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/entry.mjs",
      "require": "./dist/entry.cjs"
    }
  },
  "dependencies": {
    "@nestjs/mapped-types": "*"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^22.10.7",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "zod": "^4.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/jwt": "^11.0.1",
    "passport-jwt": "^4.0.1",
    "@nestjs/passport": "^11.0.5",
    "rxjs": "^7.8.2"
  }
}
