{
 "name": "@repo/user-interfaces",
  "version": "0.0.0",
  "license": "MIT",
   "scripts": {
  "build:cjs": "tsc --project tsconfig.cjs.json",
  "build:esm": "tsc --project tsconfig.esm.json",
  "build": "npm run build:cjs && npm run build:esm"
},
  "main": "./dist/cjs/entry.js",
  "module": "./dist/esm/entry.js",
  "types": "./dist/esm/entry.d.ts",
  "files": [
    "./dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/entry.js",
      "require": "./dist/cjs/entry.js"
    }
  },
  "dependencies": {
    "@nestjs/mapped-types": "*"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^22.10.7",
    "@types/amqplib": "^0.10.8",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "zod": "^4.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/jwt": "^11.0.1",
    "passport-jwt": "^4.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/microservices": "^11.1.8",
    "rxjs": "^7.8.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@keyv/redis": "^5.1.3",
    "cache-manager": "^7.2.4",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9"
  }
}
